{
  "name": "replicator",
  "description": "A speedy replacement-baking processor",
  "version": "0.1.0",
  "main": "build/src/client.js",
  "bin": "build/src/replicator-engine",
  "files": [
    "build/src/*.js",
    "build/src/replicator-engine"
  ],
  "scripts": {
    "lint": "bash ./script/lint",
    "test": "bash ./script/test",
    "test:functional": "ava -v ./test/functional/**/*.js",
    "clean": "rm -rf ./build",
    "build": "tsc --build tsconfig.json",
    "package": "bash ./script/package"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@types/async": "^3.0.8",
    "@types/js-yaml": "^3.12.2",
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "2",
    "@typescript-eslint/parser": "^2.23.0",
    "ava": "^3.5.0",
    "eslint": "6",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "9",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "fs-extra": "^9.0.0",
    "nyc": "^15.0.0",
    "tmp-promise": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "ava": {
    "extensions": ["js", "ts"],
    "require": ["ts-node/register"]
  },
  "dependencies": {
    "async": "^3.2.0",
    "js-yaml": "^3.13.1",
    "worker-farm": "^1.7.0"
  }
}
